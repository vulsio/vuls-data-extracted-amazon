	"advisories": [
		{
			"content": {
				"id": "ALAS-2017-844",
				"title": "Amazon Linux AMI 2014.03 - ALAS-2017-844: critical priority package update for glibc",
				"description": "Package updates are available for Amazon Linux AMI that fix the following vulnerabilities:\nCVE-2017-1000366:\n\tGlibc contains a vulnerability that allows specially crafted LD_LIBRARY_PATH values to manipulate the heap/stack, causing them to alias, potentially resulting in arbitrary code execution. Please note that additional hardening changes have been made to glibc to prevent manipulation of stack and heap memory but these issues are not directly exploitable, as such they have not been given a CVE. This affects glibc 2.25 and earlier.\nCVE-2017-1000366\n",
				"severity": [
					{
						"type": "vendor",
						"source": "linux-security@amazon.com",
						"vendor": "critical"
					}
				],
				"references": [
