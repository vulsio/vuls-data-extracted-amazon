	"advisories": [
		{
			"content": {
				"id": "ALAS-2016-674",
				"title": "Amazon Linux AMI 2014.03 - ALAS-2016-674: medium priority package update for samba",
				"description": "Package updates are available for Amazon Linux AMI that fix the following vulnerabilities:\nCVE-2015-7560:\n\tA flaw was found in the way Samba handled ACLs on symbolic links. An authenticated user could use this flaw to gain access to an arbitrary file or directory by overwriting its ACL.\n1309992: \nCVE-2015-7560 samba: Incorrect ACL get/set allowed on symlink path\n",
				"severity": [
					{
						"type": "vendor",
						"source": "linux-security@amazon.com",
						"vendor": "medium"
					}
				],
				"references": [
					{
